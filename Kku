public class kku{
    public String id;
    public String name;
    public String classroom;

    public kku() {
        this.id = "";
        this.name = "";
        this.classroom = "";
    }
    
    public kku(String id, String name, String classroom) {
        this.id = id;
        this.name = name;
        this.classroom = classroom;
    }

    public String toString() {
        return "id: " + id + ", name: " + name + ", class: " + classroom;
    }
    
    public String getId() {
        return id;
    }
    
    public String getName() {
        return name;
    }
    
    public String getClassroom() {
        return classroom;
    }
  
    public void setId(String id) {
        this.id = id;
    }

    public void setName(String name) {
        this.name = name;
    }
    
    public void setClassroom(String classroom) {
        this.classroom = classroom;
    }

    public void show()
    {
        System.out.println("id: " + id + ", name: " + name + ", class: " + classroom);
    }

  public static void main(String[] args) {
    kku n = new kku();
    n.setId("6533800169-7");
    n.setName("Natwarawit");
    n.setClassroom("9228");
    n.show();
    }
}